@{
    ViewBag.Title = "DynamicEvent";
    Layout = "~/Views/Shared/AdminZone.cshtml";
}

<html>
    <body>
        <div class="container-fluid">
            <div class="row">
            <div class="col-sm-3"></div>
            <div class="col-sm-6 panel panel-body" style="background:black; margin-top:2%;">
                 <form action="/Home/DynamicEvent" class="was-validated panel panel-body" method="post" enctype="multipart/form-data" style="background:white;">
                      <div class="form-group form-check">
                            <label>Upload New Events</label>
                            <textarea class="form-control" rows="3" id="message" name="message" required></textarea>
                      </div>
                     <button type="submit" class="btn btn-primary btn btn-block" style="background:blue;">Upload</button>
                 </form>
            </div>
            <div class="col-sm-3"></div>
        </div>
            <div class="row">
                <div class="col-sm-3"></div>
                <div class="col-sm-6" style="margin-top:1%;">
                    <form action="/Home/Admin">
                     <button class="btn btn-success btn btn-block">Go Back</button>
                    </form>
                </div>
                <div class="col-sm-3"></div>
            </div>
        </div>
    </body>
</html>
